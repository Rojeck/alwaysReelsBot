generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AuditEvents {
  id        Int      @id @default(autoincrement())
  postUrl   String
  userTag   String?
  groupName String?
  groupType String?
  groupId   String
  createdAt DateTime @default(now()) @map("created_at")
}
